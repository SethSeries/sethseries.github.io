body {
	background-color: #FFF5EE;
}

header {
	background-color: white;
	padding: 5% 8% 5% 5%;
	margin-left: 0px;
	margin-bottom: 2em;
	border-bottom: 2px solid slategrey;
	text-align: center;
}

h1 {
  font-family:'Times New Roman', Times, serif;
  font-weight: 800;
  font-size: 3.5em;
  color: slategrey;
  text-align: center;
 }
  
h2 {
  font-family:'Times New Roman', Times, serif;
  font-weight: bold;
  font-size: 2.0em;
  color: slategrey;
  text-align: center;
 }

h3 {
  font-family:'Times New Roman', Times, serif;
  font-weight: 700;
  font-size: 1.5em;
  color: black;
 }

ul {
  font-weight: 500;
  font-size: 1.2em;
  color: black;
 }

#nav li {
  float: left;
  width: 32.75%;
  margin: .5% .25% .5% .25%;
  text-align: center;
}

#nav ul li a {
  text-decoration: none;
  color: white;
  padding: 1em;
  display: block;
  background-color: slategrey;
  border: 1px solid slategrey;
  border-color:black;
  border-width:2px;
}

#nav ul {
  list-style-type: none;
  padding: 0;
  margin: 0;
  overflow: hidden;
  font-family: 'Times New Roman', Times, serif;
  font-weight: 900;
  background-color: white;
}

#nav ul li a:hover {
  background-color: white;
  border: 1px solid black;
  color: black;
}
.photo {
  background-color: white;
  text-align: center;
  border: 5px solid;
  border-radius: 20px;
  border-color: #F6FFCF;
  width: 800px;
  height: 600px;
  display: block;
  margin-left: auto;
  margin-right: auto;
  margin-bottom: 60px;
}
